class Program
{
    public static void Main(string[] args)
    {
    		int[] vetor = {10,84,75,46,50};
    	int maior = vetor.Min();
    	
    	Console.WriteLine("O menor número é: " + maior);
    		
    	
        Console.Write("Press any key to continue . . . ");
        Console.ReadKey(true);
  
        }
    } 
}
  
